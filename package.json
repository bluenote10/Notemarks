{
  "name": "LabNotes",
  "version": "0.1.0",
  "description": "Simple note taking app.",
  "main": "build/main.js",
  "scripts": {
    "build_main": "nim js -d:node -o:./build/main.js ./src/app_main.nim",
    "build_renderer": "nim js -o:./build/renderer.js ./src/app_renderer.nim",
    "build_css": "node-sass --omit-source-map-url styles/styles.scss build/styles.css",
    "start": "npm run build_main && npm run build_renderer && npm run build_css && electron ."
  },
  "keywords": [
    "Electron",
    "Nim",
    "Note taking"
  ],
  "author": "Fabian Keller",
  "license": "MIT",
  "devDependencies": {
    "bulma": "^0.7.4",
    "electron": "^4.0.4",
    "node-sass": "^4.11.0",
    "showdown": "^1.9.0"
  },
  "dependencies": {
    "glob": "^7.1.3",
    "js-yaml": "^3.12.1",
    "mousetrap": "^1.6.2"
  }
}
